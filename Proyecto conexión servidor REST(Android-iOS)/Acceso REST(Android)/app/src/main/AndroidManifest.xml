<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="juanpomares.persistencia.mastermoviles.videoclub">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="juanpomares.persistencia.mastermoviles.videoclub.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="juanpomares.persistencia.mastermoviles.videoclub.MovieDetalleActivity"
            android:parentActivityName="juanpomares.persistencia.mastermoviles.videoclub.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="juanpomares.persistencia.mastermoviles.videoclub.MainActivity" />

        </activity>
    </application>

</manifest>
